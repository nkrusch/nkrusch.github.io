{
  "name": "my_website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run copy_public && next dev",
    "start": "next start",
    "copy_public": "rm -rf public && cp -r ../public public",
    "build": "npm run copy_public && next build && next-sitemap --config next-sitemap.js && next export",
    "clean": "rm -rf out && rm -rf .next"
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "date-fns": "^2.30.0",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "next": "^14.0.0",
    "next-sitemap": "^4.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rehype-stringify": "^10.0.0",
    "remark-basepath": "^1.0.1",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "unified": "^11.0.4"
  }
}
