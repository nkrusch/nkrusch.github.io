{
  "name": "my_website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run copy_public && next dev",
    "start": "next start",
    "copy_public": "rm -rf public && cp -r ../public public",
    "build": "npm run copy_public && next build && next-sitemap --config next-sitemap.js && next export",
    "clean": "rm -rf out && rm -rf .next"
  },
  "devDependencies": {
    "@types/node": "^20.4.10",
    "date-fns": "^2.30.0",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "next": "^13.4.13",
    "next-sitemap": "^4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rehype-stringify": "^9.0.3",
    "remark-basepath": "^1.0.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "unified": "^10.1.2"
  }
}
